{
  "name": "colorvariablesexporter",
  "description": "Export Color Variables for development",
  "version": "0.3.1",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "ColorVariablesExporter",
    "manifest": "src/manifest.json",
    "main": "colorvariablesexporter.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "repository": "https://github.com/griffin-stewie/ColorVariablesExporter.git",
  "author": "griffin-stewie",
  "dependencies": {
    "change-case": "^4.1.1"
  }
}
